{
    "manifest_version": 2,
    "name": "Hyperlink Preview",
    "version": "0.3",

    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.3.1.min.js","content.js"]

        }
    ],
    "browser_action": {
        "default_title": "Toggle Hyperlink Preview",
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "update_url":"http://0.0.0.0"
}
